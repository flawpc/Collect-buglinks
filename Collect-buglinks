javascript:TiMy='';for(i=0;i<document.links.length;++i){L=document.links[i];TiMx=L.href;TiMa=L.innerHTML;if(TiMx.substring(0,44)=='http://bugzilla.mozilla.org/show_bug.cgi?id=' && TiMx!=document.location.href && TiMa!='First' && TiMa!='Last' && TiMa!='Prev' && TiMa!='Next')TiMy+=','+parseInt(TiMx.substring(44,61))} if(TiMy.length)document.location.href='http://bugzilla.mozilla.org/buglist.cgi?bugidtype=include&order=bugs.bug_id&bug_id='+TiMy; else alert('no buglinks on this page');
